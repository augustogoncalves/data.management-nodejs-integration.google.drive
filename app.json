{
  "name": "Forge.GoogleDrive",
  "description": "Basic integration between Data Management API & Google Drive",
  "repository": "https://github.com/autodesk-forge/data.management-nodejs-integration.google.drive",
  "logo": "https://avatars0.githubusercontent.com/u/8017462?v=3&s=200",
  "keywords": [
    "express",
    "framework",
    "autodesk",
    "forge",
    "template"
  ],
  "env": {
    "FORGE_CLIENT_ID": {
      "description": "Forge Client ID"
    },
    "FORGE_CLIENT_SECRET": {
      "description": "Forge Client Secret"
    },
    "FORGE_CALLBACK_URL": {
      "description": "Callback URL of your Forge app, required for 3-legged OAuth",
      "value": "https://<<YOUR HEROKU APP NAME HERE>>.herokuapp.com/api/forge/callback/oauth"
    },
    "GOOGLE_CLIENT_ID": {
      "description": "Google Client ID"
    },
    "GOOGLE_CLIENT_SECRET": {
      "description": "Google Client Secret"
    },
    "GOOGLE_CALLBACK_URL": {
      "description": "Redirect URI of your Google app, required for 3-legged OAuth",
      "value": "https://<<YOUR HEROKU APP NAME HERE>>.herokuapp.com/api/google/callback/oauth"
    }
  },
  "website": "https://developer.autodesk.com/",
  "success_url": "/"
}